{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "Node",
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "es2017"
    ],
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
    },
    "jsx": "react-jsx"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
